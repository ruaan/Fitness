<h1>Listing magentopushes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @magentopushes.each do |magentopush| %>
      <tr>
        <td><%= link_to 'Show', magentopush %></td>
        <td><%= link_to 'Edit', edit_magentopush_path(magentopush) %></td>
        <td><%= link_to 'Destroy', magentopush, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Magentopush', new_magentopush_path %>
